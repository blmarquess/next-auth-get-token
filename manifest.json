{
  "manifest_version": 3,
  "name": "Next Auth Get Token",
  "version": "1.0.0",
  "description": "Get accessToken from session requests",
  "permissions": [
    "activeTab",
    "debugger"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Get Token"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
